package javaBasic.ch02.sec10;

public class DataType2 {

	public static void main(String[] args) {
		// 논리형 boolean 불린형 참과 거짓
		boolean b1 = true;
		boolean b2 = false;
		
		boolean b3 = 10 < 8;
		System.out.println(b3);
		
		boolean b4 = 100 != 100;
		System.out.println(b4);
		
		//"국제컴퓨터학원" - 문자열 여러글자(1글자이상) 클래스형 (참조형)으로 선언하기
		String acadyname = "국제컴퓨터학원";
		
		
	
	
	
	}

}
