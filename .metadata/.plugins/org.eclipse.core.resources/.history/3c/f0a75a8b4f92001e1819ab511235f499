package javaBaisc.ch01.sec12;

public class Calculator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 1;           //변수 x를 선언하고 1을 저장
 		int y = 2;           //변수 y를 선언하고 2를 저장
 		int z = 3;           //변수 z를 선언하고 3를 저장
 		int b = 4;            //변수 b를 선언하고 4를 저장
		int result = x + y + z + b;  //변수 result를 선언하고 x와 y, z, b를 더한 값을 저장
		System.out.println(result);  //콘솔에 출력하는 메소드 호출
	}

}



public class Calculator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//형식 변수형태 변수명 = 값;
		String name:
		name = "홍길동";
		
	   int basicPay=50000; //급여
	   String phoneNumber = "010-3333-4444"; //전화번호
	   String accountNumber = "국민 123-1111-1234"; //계좌번호
	   String dept = "알바생"; //직급
	   boolean sex = true; //성별 남자
	   double tex = 0.03; //세금 비율
	
	}

}
