		   String dept = "알바생"; //직급
		   boolean sex = true; //성별 남자
		   double tex = 0.03; //세금 비율
		   
		   System.out.println("1번 알바정도 : "+ name+ "  " + basicPay + "  " + phoneNumber);
		   System.out.println(accountNumber + "  " + dept + "  " + sex + "  " + tex);
		   
	
		  // TODO Auto-generated method stub
				  String name2 = "전우치";
				
					
