package javaBasic.ch01.src15;

public class DataTypeEX {

	public static void main(String[] args) {
		// TODO Auto-generated method stub     
	   // 자료형 - 기본형, 참조형
		
		
	   byte b1 = 30;
	   System.out.println( b1 );
	   
	   byte b2 = 10;
	   byte b3 = 20;
	   
	   int b23 = b2 + b3; // + 더하기 연산자는 int형으로 처리함  byte 형 int 형 byte 형
			   
	}

}
