package javaBasic.ch01.src15;

public class DataTypeEX {

	public static void main(String[] args) {
		// TODO Auto-generated method stub     
	   // 자료형 - 기본형, 참조형
		
		
	   byte b1 = 30;
	   System.out.println( b1 );
	   
	   byte b2 = 10;
	   byte b3 = 20;
	   
	   int b2b3 = b2 + b3; // + 더하기 연산자는 int형으로 처리함  byte 형 int 형 byte 형
	
	   
	   byte b4 = 20;
	   short s4= 30;
	   
	   long b4s4 = b4 + s4;
	   
	   float f1 = 3.14f;
	   float f2 = 3.12345678f;
	   
	   System.out.println(f1 + "   " + f2);
	   
	   double d1 = 3.123456789012345;
	   System.out.println( d1 );
	   
	   
	   
	   
	}

}
